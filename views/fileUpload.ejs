<!DOCTYPE html>
<!-- Coding By CodingNepal - youtube.com/codingnepal -->
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Report Diagnostics</title>
    <link rel="stylesheet" href="css/fileUpload.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>

<body>
  <header class="header">

      <a href="/" class="logo"><img src="image/logo.png" width="200" height="50" style="float:left"></a>

      <nav class="navbar">
          <a href="/report">Check Report</a>
          <a href="#book">Book appointment</a>
          <a href="/store">E-Store</a>
          <a href="/diet">AI Nutritionist</a>
          <a href="#blogs">AI Therapist</a>
          <% if(user === "NULL") { %>
                <a href="/login">Login</a>
         <% } else { %>

           <div class="dropdown show">
       <a class="dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
         <%= user %>
       </a>

       <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
         <button class="btn-lg dropdown-item" type="button">Profile</button>
         <button class="btn-lg dropdown-item" type="button">Appointments</button>
            <button class="btn-lg dropdown-item" type="button">Diets</button>
               <div class="dropdown-divider"></div>
         <button class="btn-lg dropdown-item"   onclick="window.location.href = '/logout';" type="button">Log Out</button>
       </div>
     </div>

         </nav>


         <!-- <div id="menu-btn" class="fas fa-bars"></div> -->

               <!-- <a href="/"><%= user %></a> -->
        <%  } %>



  </header>
    <img class="doctorImage" src="image/Online report-bro.svg" width="500" height="600">

    <div class="wrapper">
        <header>Upload Your Report !</header>
        <form action="/pdf"  enctype="multipart/form-data" method="post">
            <input class="file-input" type="file" name="pdf" hidden >
            <i class="fas fa-cloud-upload-alt"></i>
            <p>Browse File to Upload</p>
            
            <button class="btn btn-outline-secondary" type="submit" id="inputGroupFileAddon04">Submit</button>
        </form>
        <section class="progress-area"></section>
        <section class="uploaded-area"></section>
    </div>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="js/script.js"></script>
    <script src="js/fileUploadScript.js"></script>

</body>

</html>
